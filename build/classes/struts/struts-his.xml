		</action>
		<action name="dzcf_*" class="dzcfAction" method="{1}">
			<!-- <result name="editYp">/his/ypList.jsp</result>
			<result name="add">/his/addChufang.jsp</result> -->
			<result name="add">/his/ypList.jsp</result>
			<result name="detail">/his/ypDetail.jsp</result>
			<result name="list">/his/chufangList.jsp</result>
		</action>
	</package>
</struts>
