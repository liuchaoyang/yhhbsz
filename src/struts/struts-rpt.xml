<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="rpt_action" extends="struts-default" namespace="/rpt">
		<action name="anaRpt_*" class="analysisReportAction" method="{1}">
			<result name="bpAnaRptView">/rpt/bpAnaRptView.jsp</result>
			<result name="bpAnaRptPrint">/rpt/bpAnaRptPrint.jsp</result>
			<result name="bsAnaRptView">/rpt/bsAnaRptView.jsp</result>
			<result name="bsAnaRptPrint">/rpt/bsAnaRptPrint.jsp</result>
			<result name="custRpt">/rpt/myAnalysisReports.jsp</result>
		</action>
		<action name="checkReport_*" class="checkReportAction" method="{1}">
			<result name="view">/rpt/checkRptView.jsp</result>
		</action>
	</package>
</struts>
