<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.yzxt.yh.module.pwd.bean.FindPwd" table="sys_findpwd"
		catalog="">
		<id name="id" type="java.lang.String">
			<column name="id" length="50" />
			<generator class="uuid.hex" />
		</id>
		<property name="userId" type="java.lang.String">
			<column name="user_id" length="50">
			</column>
		</property>
		<property name="validateCode" type="java.lang.String">
			<column name="validate_code" length="50">
			</column>
		</property>
		<property name="outDate" type="java.sql.Timestamp">
			<column name="out_date" length="19">
			</column>
		</property>
	</class>
</hibernate-mapping>
