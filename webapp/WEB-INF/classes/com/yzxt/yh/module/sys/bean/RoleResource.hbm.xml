<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.yzxt.yh.module.sys.bean.RoleResource" table="sys_role_resource"
		catalog="">
		<id name="id" type="java.lang.String">
			<column name="id" length="36" />
			<generator class="uuid.hex" />
		</id>
		<property name="roleId" type="java.lang.String">
			<column name="role_id" length="36" not-null="true">
			</column>
		</property>
		<property name="resourceId" type="java.lang.String">
			<column name="resource_id" length="36" not-null="true">
			</column>
		</property>

	</class>
</hibernate-mapping>
