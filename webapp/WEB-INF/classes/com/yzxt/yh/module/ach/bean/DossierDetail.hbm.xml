<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.yzxt.yh.module.ach.bean.DossierDetail" table="ach_dossier_detail"
		catalog="">
		<id name="id" type="java.lang.String">
			<column name="id" length="36" />
			<generator class="uuid.hex" />
		</id>
		<property name="dossierId" type="string">
			<column name="dossier_id" length="36" not-null="true">
			</column>
		</property>
		<property name="fileId" type="string">
			<column name="file_id" length="36" not-null="true">
			</column>
		</property>
		<property name="seq" type="int">
			<column name="seq" not-null="true">
			</column>
		</property>
	</class>
</hibernate-mapping>
